<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Day2_Xamarin_ITI.CreatePage"
            NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout >
            <Entry x:Name="nm" Placeholder="Name of Dish"></Entry>
            <Entry x:Name="im" Placeholder="Image File Name"></Entry>
            <Entry x:Name="pr" Keyboard="Numeric" Placeholder="Price"></Entry>
            <Button Clicked="Button_Clicked"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>