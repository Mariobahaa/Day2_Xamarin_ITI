<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Day2_Xamarin_ITI.Login"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout >
            <Entry x:Name="nm" Placeholder="Name"></Entry>
            <Entry x:Name="ps" IsPassword="True" Placeholder="Password"></Entry>
            <Button Clicked="Button_Clicked" Text="Login" BackgroundColor="DeepSkyBlue"></Button>
            <!--<Button Clicked="Button_Clicked_1" Text="Back" BackgroundColor="DeepSkyBlue"></Button>-->

        </StackLayout>
    </ContentPage.Content>
</ContentPage>